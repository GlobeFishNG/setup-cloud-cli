{
  "name": "setup-cloud-cli",
  "version": "1.0.1",
  "private": true,
  "description": "Setup Cloud CLI",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt && ncc build src/cleanup/index.ts --source-map --license licenses.txt -o dist/cleanup",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:GlobeFishNG/setup-cloud-cli.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "GlobeFishNG",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.1.1",
    "@octokit/webhooks": "^7.11.2",
    "aws-sdk": "^2.1046.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.4",
    "@typescript-eslint/parser": "^4.1.0",
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}
